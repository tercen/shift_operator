{
  "name": "Shift",
  "description": "Adds (or subtracts) a constant number from the selected range of data.",
  "tags": [
    "arithmetic"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/shift_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "constant",
      "defaultValue": 0,
      "description": "Shift value"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Values to shift",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecs": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Row",
                "Column"
              ],
              "rColumns": [
                "Row",
                "Column"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "value",
                  "type": "double"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}
