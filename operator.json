{
  "name": "Shift",
  "description": "Adds (or subtracts) a constant number from the selected range of data.",
  "tags": [
    "arithmetic"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/shift_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "constant",
      "defaultValue": 0,
      "description": "Shift value"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "data",
                "type": "numeric",
                "description": "values as input to the shift operator",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}